{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bao-loc": {
      "projectType": "application",
      "cli": {
        "analytics": false,
        "defaultCollection": "@ionic/angular-toolkit"
      },
      "schematics": {
        "@ionic/angular-toolkit:page": {
          "style": "scss"
        },
        "@ionic/angular-toolkit:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/bao-loc",
      "sourceRoot": "projects/bao-loc/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bao-loc",
            "index": "projects/bao-loc/src/index.html",
            "main": "projects/bao-loc/src/main.ts",
            "polyfills": "projects/bao-loc/src/polyfills.ts",
            "tsConfig": "projects/bao-loc/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/bao-loc/src/favicon.ico",
              "projects/bao-loc/src/assets",
              { "glob": "**/*.svg", "input": "projects/bao-loc/node_modules/ionicons/dist/ionicons/svg", "output": "./svg" }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/bao-loc/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/bao-loc/src/environments/environment.ts",
                  "with": "projects/bao-loc/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "bravo-ch4mp": {
              "browserTarget": "bao-loc:build:development",
              "host": "bravo-ch4mp",
              "ssl": true,
              "sslCert": "C:/Users/ch4mp/.ssh/bravo-ch4mp_self_signed.crt",
              "sslKey": "C:/Users/ch4mp/.ssh/bravo-ch4mp_req_key.pem"
            },
            "production": {
              "browserTarget": "bao-loc:build:production"
            },
            "development": {
              "browserTarget": "bao-loc:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bao-loc:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bao-loc/src/test.ts",
            "polyfills": "projects/bao-loc/src/polyfills.ts",
            "tsConfig": "projects/bao-loc/tsconfig.spec.json",
            "karmaConfig": "projects/bao-loc/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/bao-loc/src/favicon.ico",
              "projects/bao-loc/src/assets",
              { "glob": "**/*.svg", "input": "projects/bao-loc/node_modules/ionicons/dist/ionicons/svg", "output": "./svg" }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/bao-loc/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@c4-soft/solutions-api": {
      "projectType": "library",
      "root": "projects/c4-soft/solutions-api",
      "sourceRoot": "projects/c4-soft/solutions-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/c4-soft/solutions-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/c4-soft/solutions-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/c4-soft/solutions-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/c4-soft/solutions-api/src/test.ts",
            "tsConfig": "projects/c4-soft/solutions-api/tsconfig.spec.json",
            "karmaConfig": "projects/c4-soft/solutions-api/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "bao-loc"
}
